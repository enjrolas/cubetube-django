$(document).ready(function(){

    $('.photoDetail').slick({
	slidesToShow: 1,
	slidesToScroll: 1,
	arrows: false,
	fade: true,
	asNavFor: '.photoNav'
    });
    $('.photoNav').slick({
	slidesToShow: 3,
	slidesToScroll: 1,
	asNavFor: '.photoDetail',
	dots: true,
	centerMode: true,
	focusOnSelect: true
    });
});